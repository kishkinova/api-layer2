                "zowe-install/src/main/resources/component-scripts/start.sh": {
                    "target": "bin/start.sh",
                    "binary": false
                },
                "keystore/localhost/localhost.keystore.p12": {
                    "target": "keystore/keystore.p12",
                    "binary": true
                },
                "keystore/localhost/localhost.truststore.p12": {
                    "target": "keystore/truststore.p12",
                    "binary": true
                }
            }
        }
    }

