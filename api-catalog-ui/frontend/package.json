{
    "name": "frontend",
    "version": "0.3.0",
    "private": true,
    "homepage": ".",
    "dependencies": {
        "@emotion/core": "10.1.1",
        "@emotion/is-prop-valid": "1.1.0",
        "@emotion/styled": "10.0.27",
        "emotion-theming": "10.0.27",
        "eslint": "7.11.0",
        "eslint-config-airbnb": "16.1.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "exception-formatter": "1.0.7",
        "history": "4.7.2",
        "install": "0.12.2",
        "lodash": "4.17.20",
        "loglevel": "1.6.1",
        "mineral-ui": "0.58.0",
        "mineral-ui-icons": "0.5.0",
        "npm": "7.5.4",
        "react": "16.6.3",
        "react-app-polyfill": "0.1.3",
        "react-dom": "16.6.3",
        "react-loadable": "5.5.0",
        "react-redux": "5.1.1",
        "react-render-html": "0.6.0",
        "react-router-dom": "4.3.1",
        "react-router-tabs": "1.2.0",
        "react-scripts": "4.0.2",
        "react-toastify": "4.4.3",
        "redux": "4.0.1",
        "redux-catch": "1.3.1",
        "redux-logger": "3.0.6",
        "redux-observable": "1.0.0",
        "redux-persist": "5.10.0",
        "redux-persist-transform-filter": "0.0.18",
        "redux-thunk": "2.3.0",
        "rxjs": "6.3.3",
        "swagger-ui-react": "3.25.0",
        "uuid": "3.3.2"
    },
    "scripts": {
        "build": "npm run coverage && npm run react-scripts-build",
        "react-scripts-build": "react-scripts build",
        "postbuild": "rimraf build/**/*.map",
        "test": "react-scripts test --env=jsdom components/* reducers/* epics/* actions/* selectors/* ErrorBoundary/* --reporters=default --reporters=jest-html-reporter",
        "coverage": "npm test -- --coverage --watchAll=false",
        "cy:open": "cypress open",
        "cy:e2e:ci": "cypress run --spec \"cypress/integration/e2e/**/*.test.js\" --browser firefox --headless --env catalogHomePage=${API_CATALOG_BASE_URL},username=${MF_USERID},password=${MF_PASSWORD}",
        "cy:e2e:zowe-ci": "cypress run --spec \"cypress/integration/e2e/**/*.test.js\" --browser firefox --headless",
        "cy:e2e:localhost": "cypress run --spec \"cypress/integration/e2e/**/*.test.js\"",
        "cy:e2e:mocked-backend": "cypress run --spec \"cypress/integration/mocked-e2e/**/*.test.js\" --env catalogHomePage=https://localhost:3000/ui/v1/apicagalog,username=user,password=user",
        "the-scripts-below-might-be-stale": "",
        "start": "cross-env HTTPS=true react-scripts start",
        "start:dev": "concurrently -c blue,green -n Backend,UI \"npm run start:mock\" \"cross-env REACT_APP_GATEWAY_URL=http://localhost:8000 npm run start\"",
        "start:mock": "cross-env REACT_APP_GATEWAY_URL=http://localhost:8000 nodemon mock/server.js",
        "analyze": "source-map-explorer build/static/js/main.*",
        "delete:reports": "rm ./test-results/e2e/* || true"
    },
    "proxy": "https://localhost:10010",
    "jest-html-reporter": {
        "pageTitle": "API Catalog UI Unit Tests",
        "outputPath": "./test-results/test-report-unit.html",
        "includeFailureMsg": true
    },
    "devDependencies": {
        "body-parser": "1.18.3",
        "concurrently": "5.3.0",
        "cors": "2.8.5",
        "cross-env": "5.2.0",
        "cypress": "6.5.0",
        "enzyme": "3.7.0",
        "enzyme-adapter-react-16": "1.15.6",
        "eslint-config-prettier": "3.3.0",
        "eslint-plugin-flowtype": "2.50.3",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-prettier": "2.7.0",
        "eslint-plugin-react": "7.11.1",
        "express": "4.16.4",
        "jest": "26.6.0",
        "jest-environment-enzyme": "7.1.2",
        "jest-enzyme": "7.0.1",
        "jest-html-reporter": "3.3.0",
        "jest-junit": "6.4.0",
        "jest-mock": "23.2.0",
        "nodemon": "1.18.6",
        "prettier": "1.14.2",
        "react-testing-library": "5.3.0",
        "rimraf": "3.0.2",
        "source-map-explorer": "2.3.1",
        "start-server-and-test": "1.10.8"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/App.{jsx,js}",
            "src/**/*.{jsx,js}",
            "src/**/reducers/*.{jsx,js}",
            "!src/index.js",
            "!src/**/*Container.jsx",
            "!src/responsive-tests/**",
            "!cypress/*"
        ]
    },
    "engines": {
        "node": "=10.23.3"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
