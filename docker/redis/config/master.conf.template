requirepass "heslo"

{TLS_SETTING}tls-cert-file /config/keystore/localhost/localhost.keystore.cer
{TLS_SETTING}tls-key-file /config/keystore/localhost/localhost.keystore.key
{TLS_SETTING}tls-ca-cert-file /config/keystore/local_ca/zowe-dev-ca.cer
{TLS_SETTING}tls-auth-clients no
{TLS_SETTING}tls-replication yes
# overwrites any port directive above
{TLS_SETTING}port 0
{TLS_SETTING}tls-port {TLS_PORT}
