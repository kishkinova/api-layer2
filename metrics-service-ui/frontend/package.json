{
    "name": "metrics-frontend",
    "version": "0.3.0",
    "private": true,
    "homepage": ".",
    "dependencies": {
        "@babel/core": "7.14.2",
        "@material-ui/core": "4.11.4",
        "eslint": "7.11.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-scripts": "4.0.3"
    },
    "devDependencies": {
        "eslint-config-airbnb": "16.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-flowtype": "5.7.2",
        "eslint-plugin-import": "2.23.2",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.23.2",
        "jest-html-reporter": "3.4.0",
        "prettier": "2.3.0"
    },
    "scripts": {
        "build": "react-scripts build",
        "test": "react-scripts test --silent --watchAll=false --env=jsdom --reporters=default --reporters=jest-html-reporter --coverage --passWithNoTests",
        "start": "react-scripts start"
    },
    "jest-html-reporter": {
        "pageTitle": "Metrics Service UI Unit Tests",
        "outputPath": "./test-results/test-report-unit.html",
        "includeFailureMsg": true
    },
    "engines": {
        "node": "=10.23.3"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
