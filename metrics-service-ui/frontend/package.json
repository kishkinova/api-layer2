    "dependencies": {
        "@babel/core": "7.14.2",
        "@material-ui/core": "4.11.4",
        "@material-ui/icons": "4.11.2",
        "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
        "axios": "0.24.0",
        "eslint": "7.32.0",
        "history": "4.10.1",
        "lodash": "4.17.21",
        "loglevel": "1.7.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-loadable": "5.5.0",
        "react-redux": "7.2.4",
        "react-router-dom": "5.2.0",
        "react-scripts": "4.0.3",
        "redux": "4.1.0",
        "redux-catch": "1.3.1",
        "redux-logger": "3.0.6",
        "redux-observable": "1.2.0",
        "redux-persist": "6.0.0",
        "redux-persist-transform-filter": "0.0.20",
        "redux-thunk": "2.3.0",
        "rxjs": "6.6.7",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "axios-mock-adapter": "1.20.0",
        "body-parser": "1.19.2",
        "cors": "2.8.5",
        "cross-env": "7.0.3",
        "cypress": "7.4.0",
        "enzyme": "3.11.0",
        "eslint-config-airbnb": "16.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-flowtype": "5.7.2",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import": "2.23.2",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.23.2",
        "express": "4.17.3",
        "jest": "26.6.0",
        "jest-enzyme": "7.1.2",
        "jest-html-reporter": "3.4.0",
        "jest-mock": "26.6.2",
        "nodemon": "2.0.15",
        "npm-force-resolutions": "0.0.10",
        "prettier": "2.3.0"
    },
    "resolutions": {
        "ansi-html": "0.0.9",
        "ansi-regex": "5.0.1",
        "browserslist": "4.21.2",
        "ejs": "3.1.8",
        "eventsource": "1.1.2",
        "follow-redirects": "1.15.1",
        "glob-parent": "6.0.2",
        "got": "12.1.0",
        "immer": "9.0.15",
        "jsdom": "16.5.0",
        "minimist": "1.2.6",
        "node-forge": "1.3.1",
        "nth-check": "2.0.1",
        "shell-quote": "1.7.3",
        "url-parse": "1.5.10"
    },
    "scripts": {
        "preinstall": "npm-force-resolutions",
        "lint": "eslint \"src/**/*.{js,jsx}\"",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false --env=jsdom --reporters=default --reporters=jest-html-reporter --coverage --silent",
        "start": "cross-env HTTPS=true react-scripts start",
        "start:dev": "concurrently -c blue,green -n Backend,UI \"npm run start:mock\" \"cross-env REACT_APP_GATEWAY_URL=http://localhost:8000 npm run start\"",
        "start:mock": "cross-env REACT_APP_GATEWAY_URL=http://localhost:8000 nodemon mocked-backend/server.js",
        "cy:open": "cypress open",
        "cy:e2e": "cypress run --spec \"cypress/integration/e2e/**/*.test.js\" --browser firefox --headless"
    },
    "jest-html-reporter": {
        "pageTitle": "Metrics Service UI Unit Tests",
        "outputPath": "./test-results/test-report-unit.html",
        "includeFailureMsg": true
    },
    "engines": {
        "npm": "=8.1.0",
        "node": "=14.19.3"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
